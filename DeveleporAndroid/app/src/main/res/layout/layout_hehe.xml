<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_back"
        android:id="@+id/guide"
        >
    </include>
<ScrollView
    android:layout_width="wrap_content"
    android:layout_below="@+id/guide"
    android:id="@+id/src"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="match_parent"
            android:text="HHHHH"
            android:gravity="center"
            android:layout_height="wrap_content" />
        <com.example.wqq.develeporandroid.CustomView.drq
            android:layout_width="match_parent"
            android:layout_height="300dp" />
        <com.example.wqq.develeporandroid.CustomView.MyDoubleProgress
            android:layout_width="match_parent"
            android:layout_height="100dp" />
        <com.example.wqq.develeporandroid.CustomView.Clock
            android:layout_width="match_parent"
            android:layout_height="300dp" />
    </LinearLayout>

</ScrollView>


</RelativeLayout>
